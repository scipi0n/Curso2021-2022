    rr:template "http://www.calidadAire.com/{municipio}";
    rr:termType rr:IRI;
  rr:class vocab:Municipio
  ];
  
  rr:predicateObjectMap [
	rr:predicateMap [ rr:constant owl:sameAs; rr:termType rr:IRI; ];
	rr:objectMap [
